{
  "devDependencies": {
    "@types/node": "~9.4.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "lerna": "^3.13.2",
    "prettier": "1.14.0",
    "rimraf": "~2.6.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.1"
  },
  "name": "@deathbeds/jupyterlab-outsource",
  "private": true,
  "scripts": {
    "bootstrap": "jlpm",
    "build": "lerna run build --stream",
    "clean": "lerna exec --parallel --stream -- rimraf lib",
    "lint": "prettier --write *.json *.md *.yml && lerna exec -- prettier --write --config=../../.prettierrc ./src/**/*.ts ./src/**/*.tsx ./style/**/*.css ./*.json ./*.md && lerna exec -- tslint -c ../../tslint.json --fix ./src/**/*.ts",
    "watch:lab": "jupyter lab --no-browser --debug --watch",
    "watch:ts": "lerna run --scope @deathbeds/jupyterlab-outsource-meta build"
  },
  "version": "1.0.0",
  "workspaces": [
    "src/*"
  ]
}
